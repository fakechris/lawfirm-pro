{
  "dashboard": {
    "id": null,
    "title": "Law Firm Pro - Integration Services",
    "description": "Detailed monitoring for all integration services",
    "tags": ["lawfirmpro", "integrations"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"integration-services\"}",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "colorMode": "value",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total Requests by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(integration_requests_total[5m])) by (service)",
            "legendFormat": "{{service}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 16,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "PACER Service Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(integration_requests_total{service=\"pacer\"}[5m])",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "rate(integration_error_count{service=\"pacer\"}[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Stripe Service Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(integration_requests_total{service=\"stripe\"}[5m])",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "rate(integration_error_count{service=\"stripe\"}[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "PayPal Service Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(integration_requests_total{service=\"paypal\"}[5m])",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "rate(integration_error_count{service=\"paypal\"}[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Response Time by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(integration_response_time_bucket[5m])) by (service)",
            "legendFormat": "{{service}} - 95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Service Error Rates",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(integration_error_count[5m]) by (service) / rate(integration_requests_total[5m]) by (service) * 100",
            "legendFormat": "{{service}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "templating": {
      "list": []
    }
  }
}